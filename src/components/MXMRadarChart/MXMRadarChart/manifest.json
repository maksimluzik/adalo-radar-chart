{
  "displayName": "MXM Radar Chart",
  "defaultWidth": 500,
  "defaultHeight": 500,
  "description": "A customizable radar chart for visualizing data.",
  "author": "Maksim Luzik",
  "version": "0.1.0",
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "data",
      "displayName": "Data Points",
      "type": "list",
      "items": {
        "type": "number"
      },
      "default": [65, 59, 90, 81, 56, 55, 40]
    },
    {
      "name": "labels",
      "displayName": "Labels",
      "type": "list",
      "items": {
        "type": "string"
      },
      "default": ["Eating", "Drinking", "Sleeping", "Designing", "Coding", "Cycling", "Running"]
    },
    {
      "name": "backgroundColor",
      "displayName": "Background Color",
      "type": "color",
      "default": "rgba(34, 202, 236, 0.2)"
    },
    {
      "name": "borderColor",
      "displayName": "Border Color",
      "type": "color",
      "default": "rgba(34, 202, 236, 1)"
    },
    {
      "name": "title",
      "displayName": "Chart Title",
      "type": "string",
      "default": "My Radar Chart"
    }
  ]
}
